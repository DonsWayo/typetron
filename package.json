{
  "name": "typetron",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "ProductName",
    "appId": "org.your.productname",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:DonsWayo/typetron.git"
  },
  "author": {
    "name": "Your name",
    "email": "email@email.com"
  },
  "license": "SEE LICENSE",
  "bugs": {
    "url": "https://github.com/DonsWayo/typetron.git/issues"
  },
  "homepage": "https://github.com/DonsWayo/typetron",
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@hot-loader/react-dom": "16.9.0-4.12.11",
    "@types/react": "16.9.1",
    "@types/react-dom": "16.8.5",
    "@types/react-hot-loader": "4.1.0",
    "@types/webdriverio": "5.0.0",
    "@types/webpack-env": "1.14.0",
    "babel-loader": "8.0.6",
    "cross-env": "5.1.3",
    "css-loader": "3.2.0",
    "electron": "^8.2.0",
    "electron-builder": "21.2.0",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "5.0.0",
    "node-sass": "4.12.0",
    "react-hot-loader": "4.12.11",
    "sass-loader": "7.2.0",
    "source-map-loader": "0.2.4",
    "spectron": "8.0.0",
    "style-loader": "1.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "fork-ts-checker-webpack-plugin": "1.5.0",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  }
}
